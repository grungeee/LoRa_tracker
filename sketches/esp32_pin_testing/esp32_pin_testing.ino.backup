
void setup() {
  Serial.begin(115200);
  // List of GPIOs you plan to test. Modify according to your specific ESP32 model and application needs.
  int gpioPins[] = {0, 2, 4, 5, 12, 13, 14, 15, 16, 17, 18, 19, 21, 22, 23, 25, 26, 27, 32, 33};
  int numPins = sizeof(gpioPins) / sizeof(gpioPins[0]);

  for (int i = 0; i < numPins; i++) {
    pinMode(gpioPins[i], INPUT_PULLUP);  // Set each pin as input with pull-up
  }
}

void loop() {
  int gpioPins[] = {0, 2, 4, 5, 12, 13, 14, 15, 16, 17, 18, 19, 21, 22, 23, 25, 26, 27, 32, 33};
  int numPins = sizeof(gpioPins) / sizeof(gpioPins[0]);

  for (int i = 0; i < numPins; i++) {
    int pinState = digitalRead(gpioPins[i]);  // Read the pin state
    Serial.print("GPIO ");
    Serial.print(gpioPins[i]);
    Serial.print(": ");
    Serial.println(pinState == LOW ? "LOW" : "HIGH");  // Print the state to the serial monitor
  }
  delay(1000);  // Delay between readings to make the output readable
}

