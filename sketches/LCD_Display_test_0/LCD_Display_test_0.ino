// Display Frames
void drawFrame(const uint8_t *frame) {
  tft.drawBitmap(0, 0, frame, tft.width(), tft.height(), GC9A01_WHITE);
}

void loop() {
  drawFrame(bongoCatFrame1);
  delay(500);
  drawFrame(bongoCatFrame2);
  delay(500);
}
